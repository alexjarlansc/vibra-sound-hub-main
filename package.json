{
  "name": "nomix-root",
  "private": true,
  "version": "0.0.0",
  "description": "Wrapper raiz para facilitar comandos sem precisar entrar na subpasta",
  "scripts": {
    "setup": "npm --prefix vibra-sound-hub-main install",
    "dev": "npm --prefix vibra-sound-hub-main run dev",
    "build": "npm --prefix vibra-sound-hub-main run build",
    "preview": "npm --prefix vibra-sound-hub-main run preview",
  "lint": "npm --prefix vibra-sound-hub-main run lint",
  "ensure:bucket": "node vibra-sound-hub-main/scripts/ensureBucket.ts || (npm i -D tsx && npx tsx vibra-sound-hub-main/scripts/ensureBucket.ts)"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/tailwindcss": "^3.0.11",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.4.19"
  },
  "dependencies": {}
}
